{
    "version": 2,
    "builds": [
      {
        "src": "src/main.ts",
        "use": "@vercel/node"
      },
      {
        "src": "node_modules/swagger-ui-dist/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/docs/(.*)",
        "dest": "/node_modules/swagger-ui-dist/$1"
      },
      {
        "src": "/(.*)",
        "dest": "src/main.ts",
        "methods": ["GET", "POST", "PUT", "DELETE", "PATCH"]
      }
    ]
  }